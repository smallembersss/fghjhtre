<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Iframe Spawner</title>
    <style>
        button {
            margin: 20px;
        }
        iframe {
            width: 200px;
            height: 150px;
            margin: 10px;
            display: inline-block;
        }
    </style>
</head>
<body>
    <button id="start">Start Spawning Iframes</button>
    <div id="iframe-container"></div> <!-- Container to hold the iframes -->

    <script>
        let interval = 1; // Start with 1 ms
        let count = 0;

        document.getElementById('start').addEventListener('click', function() {
            const spawnIframes = () => {
                // Create a new iframe element
                const newIframe = document.createElement("iframe");
                newIframe.src = "chrome-extension://cjpalhdlnbpafiamejdnhcphjbkeiagm/manifest.json";
                
                // Append the iframe to the container in the same window
                document.getElementById("iframe-container").appendChild(newIframe);

                count++;

                if (count >= 100) { // After 10 iframes, slow down
                    count = 0; // Reset count
                    interval += 1; // Increase delay
                }

                // Continue spawning with the new interval
                setTimeout(spawnIframes, interval);
            };

            spawnIframes(); // Start the spawning
        });
    </script>
</body>
</html>
